{
  "name": "commission-junction",
  "version": "0.0.6",
  "description": "Commission Junction API Helper Methods For Node / JavaScript",
  "author": {
    "name": "Den Odell",
    "email": "denodell@me.com"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:denodell/commission-junction.git"
  },
  "bugs": {
    "url": "https://github.com/denodell/commission-junction/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-0 -d dist/ src/",
    "prepublish": "npm run compile",
    "start": "node dist/index.js",
    "test": "npm run compile && ava"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-0"
      ]
    },
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.5",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "es6-promise": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "ava-spec": "^1.0.1"
  }
}
